snippet if "if (condition) { ... }"
if (${1:true}) {
	${VISUAL}
}
endsnippet

snippet ife "if (condition) { ... } else { ... }"
if (${1:true}) {
	${VISUAL}
} else {
	$2
}
endsnippet

snippet ter " condition ? ... : ..."
${1:condition} ? ${2:${VISUAL}} : $0
endsnippet

snippet log "console.log('...', ...)"
console.log('$1', $1);
endsnippet
